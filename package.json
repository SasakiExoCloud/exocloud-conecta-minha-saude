    "sass": "^1.69.5",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --config ./prettier.config.js --write"
    ],
    "**/*.{css,scss,md,html,json}": [
      "prettier --config ./prettier.config.js --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
